search gd {
  document gd {
    field boolean type predicate {
      indexing: attribute | summary
      index {
        arity: 8
        dense-posting-list-threshold: 0.15
      }
    }
    field id type int {
      indexing: attribute | summary
    }
  }
  rank-profile default {
    summary-features: subqueries(boolean).lsb subqueries(boolean).msb
  }
  document-summary minimal {
    summary documentid type string { source: documentid }
  }
}
