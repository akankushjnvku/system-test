search cased {
  document cased {
    field title type array<string> {
      indexing: attribute | summary
      match: cased
    }
    field fstitle type array<string> {
      indexing: attribute | summary
      attribute: fast-search
      dictionary {
        hash
        cased
      }
      match: cased
    }
    field weight type int {
      indexing: summary | attribute
    }
  }
}
