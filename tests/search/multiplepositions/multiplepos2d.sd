search multiplepos2d {
  document multiplepos2d {
    field title type string {
      indexing: index | summary
    }
    field ll type array<position> {
      indexing: attribute 
    }
  }
  fieldset default {
    fields: title
  }
  rank-profile default {
    first-phase {
      expression: closeness(ll)
    }
  }
}
