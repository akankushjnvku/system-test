search doc {

  document doc {

    field text type string {
      indexing: index | summary
    }

  }

  field tokens type tensor(x[5]) {
    indexing: input text | embed | attribute | summary
  }

  rank-profile default {
    first-phase {
      expression: nativeRank
    }
    summary-features {
      query(tokens)
      attribute(tokens)
    }
  }

}
