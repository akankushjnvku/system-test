search hamming {
  document hamming {
    field id type int {
      indexing: attribute | summary
    }
    field title type string {
      indexing: index | summary
    }
    field just_x type tensor<int8>(x[16]) {
      indexing: attribute
    }
  }
  rank-profile default {
    first-phase {
      expression {
        reduce(join(query(just_x),attribute(just_x),f(a,b)(hamming(a,b))),sum,x)
      }
    }
    function output_tensor() {
      expression: join(query(just_x),attribute(just_x),f(a,b)(hamming(a,b)))
    }
    summary-features: output_tensor
  }
}
