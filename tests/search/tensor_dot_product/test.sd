search test {
  document test {
    field id type int {
      indexing: attribute | summary
    }
    field dvector type tensor(x[2]) {
      indexing: attribute | summary
    }
    field dvectorf type tensor(x[2]) {
      indexing: attribute | summary
    }
  }
  rank-profile default {
    first-phase {
      expression {
        sum(query(qvector) * attribute(dvector))
      }
    }
  }
  rank-profile usefloat {
    first-phase {
      expression {
        sum(query(qvectorf) * attribute(dvectorf))
      }
    }
  }
}
