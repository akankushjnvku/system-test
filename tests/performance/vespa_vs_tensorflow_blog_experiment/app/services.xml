<services>

    <admin version="2.0">
      <adminserver hostalias="node0" />
      <configservers>
        <configserver hostalias="node0" />
      </configservers>
    </admin>


  <container version="1.0" id="container">
    <search>
      <chain id="blog" inherits='vespa'>
        <searcher id="com.yahoo.test.ImageRemovalSearcher" bundle="tensorflow" />
        <searcher id="com.yahoo.test.UserProfileSearcher" bundle="tensorflow" />
        <searcher id="com.yahoo.test.TensorFlowSearcher" bundle="tensorflow" >
          <config name="com.yahoo.test.tf-model">
            <model>models/blog/saved</model>
          </config>
        </searcher>
      </chain>
    </search>
    <document-api/>
    <nodes>
      <node hostalias="node0"/>
    </nodes>

  </container>

  <content version="1.0" id="blog_cluster">
    <redundancy>1</redundancy>
    <documents>
      <document mode='index' type='blog_post'/>
    </documents>
    <nodes>
      <node hostalias="node1" distribution-key="0"/>
      <node hostalias="node2" distribution-key="1"/>
      <node hostalias="node3" distribution-key="2"/>
      <node hostalias="node4" distribution-key="3"/>
      <node hostalias="node5" distribution-key="4"/>
      <node hostalias="node6" distribution-key="5"/>
      <node hostalias="node7" distribution-key="6"/>
    </nodes>
  </content>

</services>
