<services>

    <admin version="2.0">
      <adminserver hostalias="node0" />
      <configservers>
        <configserver hostalias="node0" />
      </configservers>
    </admin>


  <container version="1.0" id="container">
    <model-evaluation/>
    <search>
      <chain id="blog" inherits='vespa'>
        <searcher id="com.yahoo.test.ImageRemovalSearcher" bundle="tensorflow" />
        <searcher id="com.yahoo.test.UserProfileSearcher" bundle="tensorflow" />
        <searcher id="com.yahoo.test.TensorFlowSearcher" bundle="tensorflow" >
          <config name="com.yahoo.test.tf-model">
            <model>models/blog/saved</model>
          </config>
        </searcher>
        <searcher id="com.yahoo.test.TensorFlowStatelessEvaluation" bundle="tensorflow" />
      </chain>
    </search>
    <document-api/>
    <nodes>
      <node hostalias="node0"/>
    </nodes>

  </container>

  <content version="1.0">
    <redundancy>1</redundancy>
    <documents>
      <document mode='index' type='blog_post'/>
    </documents>
    <nodes>
      <node hostalias="node0" distribution-key="0"/>
    </nodes>
  </content>

</services>
